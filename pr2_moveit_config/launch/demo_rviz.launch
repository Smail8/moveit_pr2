<launch>
  <!-- specify the planning pipeline -->
  <arg name="arm_id" default="pr2" doc="Name of the panda robot to spawn" />
  <arg name="world_name" default="/home/bmerlau/Documents/geometric-reasoning-networks/planning_ws/src/sim/worlds/example.world"/>
  <arg name="pipeline" default="ompl" />

  <!-- Gazebo specific options -->
  <arg name="gazebo_gui" default="false" />
  <arg name="paused" default="false" />

  <arg name="rviz" default="true" />

  <rosparam command="load" file="$(find pr2_moveit_config)/config/robots_config.yaml" />

  <include file="$(dirname)/demo.launch" pass_all_args="true">
    <!-- robot description is loaded by gazebo.launch, to enable Gazebo features -->
    <arg name="load_robot_description" value="true" />
    <!-- MoveItSimpleControllerManager provides ros_control's JointTrajectory controllers
         as well as GripperCommand actions -->
    <arg name="moveit_controller_manager" value="simple" /> <!-- CHECK but should OK -->
    <arg name="use_rviz" value="$(arg rviz)" />
  </include>

  <param name="move_group/capabilities" value="move_group/ExecuteTaskSolutionCapability" />
</launch>
