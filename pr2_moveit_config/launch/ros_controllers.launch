<?xml version="1.0"?>
<launch>

  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find pr2_moveit_config)/config/ros_controllers.yaml" command="load"/>

  <!-- Load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
    output="screen" args="left_arm_controller left_arm_and_torso_controller right_arm_controller right_arm_and_torso_controller arms_controller head_controller torso_controller arms_and_torso_controller whole_body_controller right_gripper_controller left_gripper_controller "/>

</launch>
